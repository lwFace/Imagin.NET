<?xml version="1.0" encoding="utf-8"?>
<Layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="Imagin.Common.Controls">
  <Root xsi:type="LayoutGroup" Orientation="Horizontal">
    <Elements>
      <Element xsi:type="LayoutGroup" Orientation="Vertical">
        <Elements>
          <Element xsi:type="LayoutPanelGroup" Height="60*">
            <Panels>
              <LayoutPanel IsSelected="true" Name="CopyPanel"/>
              <LayoutPanel Name="PasswordsPanel"/>
              <LayoutPanel Name="ConvertPanel"/>
            </Panels>
          </Element>
          <Element xsi:type="LayoutPanelGroup" Height="40*">
            <Panels>
              <LayoutPanel IsSelected="true" Name="LogPanel"/>
              <LayoutPanel Name="QueuePanel"/>
            </Panels>
          </Element>
        </Elements>
      </Element>
      <Element xsi:type="LayoutPanelGroup" MinWidth="256" MaxWidth="540" Width="340">
        <Panels>
          <LayoutPanel IsSelected="true" Name="PropertiesPanel"/>
          <LayoutPanel Name="GeneratePanel"/>
          <LayoutPanel Name="OptionsPanel"/>
        </Panels>
      </Element>
    </Elements>
  </Root>
</Layout>